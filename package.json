{
  "name": "palstala",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -c development next dev",
    "build": "dotenv -c development next build",
    "start": "dotenv -c development next start",
    "lint": "eslint .",
    "analyze": "ANALYZE=true npm run build",
    "initdb": "docker-compose up -d db && dotenv -c development prisma migrate dev",
    "test": "docker-compose up -d db && npm run test:migrate,pw",
    "test:migrate,pw": "npm run test:migrate && npm run test:pw",
    "test:pw": "NODE_ENV=test dotenv -c test playwright test",
    "test:pw:updatesnapshots": "NODE_ENV=test dotenv -c test -- playwright test --update-snapshots",
    "test:pw:debug": "PWDEBUG=1 npm run test:pw",
    "test:migrate": "NODE_ENV=test dotenv -c test prisma migrate dev",
    "test:pw:skipbuild": "SKIP_BUILD=1 npm run test:pw",
    "test:pw:skipbuild:debug": "PWDEBUG=1 npm run test:pw:skipbuild",
    "test:docker": "docker-compose run --rm tests",
    "test:docker:skipbuild": "docker-compose run --rm tests npm run test:pw:skipbuild",
    "test:docker:updatesnapshots": "docker-compose run --rm tests npm run test:pw:updatesnapshots",
    "postinstall": "npx prisma generate"
  },
  "dependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^5.0.0-beta.2",
    "@material-ui/icons": "^5.0.0-beta.1",
    "@next-auth/prisma-adapter": "^0.5.3",
    "@next/bundle-analyzer": "^11.0.1",
    "@prisma/client": "^2.28.0",
    "axios": "^0.21.1",
    "babel-plugin-superjson-next": "^0.3.0",
    "fp-ts": "^2.11.1",
    "initials": "^3.1.1",
    "io-ts": "^2.2.16",
    "next": "11.0.1",
    "next-auth": "^3.27.3",
    "nprogress": "^0.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.12.2",
    "react-markdown": "^6.0.3",
    "superjson": "^1.7.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.13.1",
    "@types/nprogress": "^0.2.0",
    "@types/react": "17.0.15",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "dotenv-cli": "^4.0.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.0.1",
    "msw": "^0.33.2",
    "prettier": "^2.3.2",
    "prisma": "^2.28.0",
    "typescript": "4.3.5"
  }
}
